{
  "extends": "tslint:recommended",
  "rules": {

    "label-position": true,
    "no-shadowed-variable": false,
    "max-line-length": [
      true,
      140
    ],



    "class-name": true,
    "comment-format": [
      true,
      "check-space",
      "check-lowercase"
    ],
    "no-consecutive-blank-lines": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast",
      "check-preblock"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ]
  }
}
